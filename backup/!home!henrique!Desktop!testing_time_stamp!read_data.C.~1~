int main(){
  ifstream f;
  f.open("0_wave0_35V00_1000ADC_Ch0_or_Ch1_cathode_on_11kV.dat", ios::binary | ios::in);

  int headbin;
  int nbytes = 4;

  for(Int_t i = 0; i<6; i++){
    f.read((char *) &headbin, nbytes);
    std::cout << headbin << std::endl;
  }
  f.read((char *) &headbin, 2);
  std::cout << headbin << std::endl;
  
}
