

void plot_together(){
  TFile *fmu = new TFile("averaged_normalized_ch1","READ");
  TGraph *gmu = (TGraph*)fmu->Get("average_normalized_ch1");
  Int_t n = gmu->GetN();
  Double_t dtime = 4;
  TH1D *hmu = TH1D("hmu","hmu",n,0,n*dtime);

  for(Int_t i = 0; i<n; i++){
    hmu->SetBinContent(i+1,*(gmu->GetY()+i))
  }

  hmu->Draw();

  
}
